{
  "name": "electron-forth",
  "version": "0.0.1",
  "description": "A forth implementation wrapped with Electron written in Typescript",
  "author": {
    "name": "Roy Crippen",
    "email": "roycrippen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roycrippen/electron-forth.git"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "forth",
    "typescript",
    "electron"
  ],
  "main": "dist-js/main-bundle.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "run-electron": "electron .",
    "start": "npm run build && npm run run-electron",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "long": "^4.0.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "@types/long": "^4.0.0",
    "@types/node": "^12.0.8",
    "@types/request": "^2.48.1",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "electron": "^4.2.4",
    "electron-builder": "20.38.4",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
  "build": {
    "appId": "electronforth",
    "linux": {
      "target": "AppImage"
    }
  }
}